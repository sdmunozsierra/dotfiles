! Srcery
*.foreground:   #fce8c3
*.background:   #1c1b19
*.cursorColor:  #fbb829

! Special for polybar
! Yellow 11 highlight
*.highlight:    #fed06e
! Red 9 highlight
*.error:        #f75341
! Blue 12 active
*.active:       #68a8e4
! Black 8 inactive
*.inactive:     #918175

! black
*.color0:       #1c1b19
*.color8:       #918175

! red
*.color1:       #ef2f27
*.color9:       #f75341

! green
*.color2:       #519f50
*.color10:      #98bc37

! yellow
*.color3:       #fbb829
*.color11:      #fed06e

! blue
*.color4:       #2c78bf
*.color12:      #68a8e4

! magenta
*.color5:       #e02c6d
*.color13:      #ff5c8f

! cyan
*.color6:       #0aaeb3
*.color14:      #53fde9

! white
*.color7:       #d0bfa1
*.color15:      #fce8c3

! Bold, Italic, Underline
*.colorBD:     #f7fcff


! st
!! REQUIRED (st.shell) to avoid vim problems
st.shell:	            /bin/bash
st.termname:	        st-256color

!! Look and Feel
st.font:                hack nerd font-12;
st.opacity:             235

!! Cursor
!! Available cursor values: 2 4 6 7 = █ _ | ☃ ( 1 3 5 are blinking versions)
st.cursorshape:         3
st.cursorthickness:     1
st.cursorblinkstyle:    1
st.blinktimeout: 500

st.bold_font:	        2
st.prompt_char:         $
!st.bellvolume:	        100 


!! URxvt
URxvt.cursorBlink: 1
!! Appearance
URxvt.termName:   rxvt-unicode
URxvt.scrollBar:  false
Xft.dpi:          96
Xft.antialias:    true
Xft.rgba:      	  rgb
Xft.hinting:      true
Xft.hintstyle:    hintslight
!! Transparency settings
URxvt.transparent:     true
URxvt.shading:         15
URxvt.depth:           32
!! URxvt.inheritPixmap: true
!! Fonts
URxvt.font:        xft:hack:pixelsize=14:antialias=true,xft:monospace
URxvt.boldFont:    xft:hack:pixelsize=14:weight=bold:antialias=true,xft:monospace
URxvt.letterSpace: 7
!! Larger history limit
URxvt.saveLines:  1000000


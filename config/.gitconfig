[core]
	editor = vim
[credential]
        # helper = cache --timeout=3600
        helper = /usr/lib/git-core/git-credential-libsecret
[gpg]
	program = gpg
[alias]
	unstage = reset HEAD --
    st = status
    co = checkout
    br = branch
    aa = add .
	comm = commit -a -S -m
	last = log -1 HEAD
    recomm = reset --soft HEAD~1
    uncommit = reset --soft HEAD~1

[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
